<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="#e9eff1" >

	<include
		android:id="@+id/SupervisorDetails"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_below="@+id/promoterCheckInFrame"
		android:layout_marginTop="35dp"
		layout="@layout/contact_supervisor_list_item" />

	<include
		android:id="@+id/promoterCheckInFrame"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_alignParentTop="true"
		layout="@layout/promoter_checkin_frame" />

	<RelativeLayout
		android:id="@+id/blankCard"
		android:layout_width="match_parent"
		android:layout_height="110dp"
		android:layout_alignParentLeft="true"
		android:layout_below="@+id/SupervisorDetails"
		android:layout_marginTop="25dp"
		android:background="@android:color/white" >
	</RelativeLayout>

</RelativeLayout>